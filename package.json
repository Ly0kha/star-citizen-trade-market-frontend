  "scripts": {
    "clean": "rimraf yarn.lock dist node_modules",
    "build": "vue-cli-service build",
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "lint:all": "yarn lint:ts && yarn lint:vue",
    "lint:ts": "eslint src --ext .ts",
    "lint:ts:cache": "eslint src --ext .ts --cache",
    "lint:vue": "eslint src --ext .vue",
    "lint:pug": "pug-lint src",
    "format": "yarn format:ts && yarn format:html && yarn format:scss && yarn format:vue",
    "format:ts": "prettier --write 'src/**/*.ts'",
    "format:pug": "prettier --write 'src/**/*.pug'",
    "format:scss": "prettier --write 'src/**/*.scss'",
    "format:vue": "prettier --write **/*.vue"
